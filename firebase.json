{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [{
      "source": "/**",
      "dynamicLinks": true
    },
    {
      "source": "!/actuaciones/**",
      "destination": "estadisticas.municipaldemairena.com"
    }]
  }
}
